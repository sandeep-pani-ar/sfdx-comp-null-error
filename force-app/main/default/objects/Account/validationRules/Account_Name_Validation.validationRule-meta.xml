<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Name_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(CONTAINS(Name, &apos;Corp&apos;))</errorConditionFormula>
