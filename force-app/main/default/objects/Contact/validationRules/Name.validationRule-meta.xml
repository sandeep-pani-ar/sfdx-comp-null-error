<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Name</fullName>
    <active>false</active>
    <errorConditionFormula>ISBLANK( FirstName ) || ISBLANK( LastName )</errorConditionFormula>
    <errorMessage>Both First Name and Last Name must not be blank.</errorMessage>
</ValidationRule>
